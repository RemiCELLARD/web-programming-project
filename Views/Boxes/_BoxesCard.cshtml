@model List<Box>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
@if (Model.Count() == 0)
{
    <partial name="_BoxesNoResult" />
}
else
{
    @foreach (Box item in Model)
    {
        <partial name="_BoxCard" model="item" />
    }
}
