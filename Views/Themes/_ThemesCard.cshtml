@model List<Theme>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

@if (Model.Count() == 0)
{
    <partial name="_ThemesNoResult" />
}
else
{
    @foreach (Theme item in Model)
    {
        <partial name="_ThemeCard" model="item" />
    }
}


