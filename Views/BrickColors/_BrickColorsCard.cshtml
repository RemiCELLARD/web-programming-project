@model List<BrickColor>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
@if (Model.Count() == 0)
{
    <partial name="_BrickColorsNoResult" />
}
else
{
    @foreach (BrickColor item in Model)
    {
        <partial name="_BrickColorCard" model="item" />
    }
}