@model List<Brick>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
@if (Model.Count() == 0)
{
    <partial name="_BrickNoResult" />
}
else
{
    @foreach (Brick item in Model)
    {
        <partial name="_BrickTableline" model="item" />
    }
}